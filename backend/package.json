{
	"name": "backend",
	"version": "1.0.0",
	"description": "An Express and React application that interacts with the iTunes Search API",
	"main": "server.js",
	"engines": {
		"node": "16.13.2",
		"npm": "8.1.2"
	},
	"scripts": {
